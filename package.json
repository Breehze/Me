{
  "name": "breehzo-fr",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "tw": "npx tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch",
    "build": "export NODE_ENV=prod && npx tailwindcss -i ./src/css/input.css -o ./src/css/output.css && npx tailwindcss -o ./src/css/build.css --minify && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "tailwindcss-animated": "^1.0.1",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.11"
  }
}
